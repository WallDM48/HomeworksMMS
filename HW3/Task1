#include<stdio.h>

int main(){
    int a=89;
    int b=054;
    int c=0xff1;
    int d=0b11110;

    //a) dec -> bin; 
    // 89 = 1011001;
    //b) bin -> dec;
    printf("%d\n",d);
    //c) dec -> hex
    printf("%x\n",a);
    // d) hex -> dec
    printf("%d\n",c);
    // e) hex -> bin
    // we have to convert every symbol : 0xff1 = 111111110001;
    // f) bin -> hex
    printf("%x\n",d);
    // g) dec -> oct
    printf("%o\n",a);
    // h) oct -> dec
    printf("%d\n",b);
    // I) we need convert into dec and then to convert to base 4

    return 0;
}